{% extends 'base.html.twig' %}

{% block title %}{{etudiant_annee_academique|upper}}{% endblock %}
{% block titrePage %}Gestion des paiements {% endblock %}
{% block cardHeader %} {{ block ('title')}} {% endblock %}
{% block body %}
<table class="table table-hover table-striped  table-bordered" id="dataTable" width="100%" cellspacing="0">
		<thead>
			<tr>
				<th>#</th>
				<th>Frais </th>
				<th>Date paiement </th>
				<th>Percepteur </th>
			</tr>
		</thead>
		<tfoot>
			<tr>
				<th>#</th>
				<th>Frais </th>
				<th>Date paiement </th>
				<th>Percepteur </th>
			</tr>
		</tfoot>
		<tbody>
			{% for paiement in etudiant_annee_academique.paiements %}
				<tr>
					<td>{{ loop.index }}</td>
					<td>{{ paiement.frais }} </td>
					<td>{{ paiement.date }}</td>
					<td>{{ paiement.utilisateur }}</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="4">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>


    {# <a href="{{ path('app_etudiant_annee_academique_index') }}">back to list</a>

    <a href="{{ path('app_etudiant_annee_academique_edit', {'id': etudiant_annee_academique.id}) }}">edit</a>

    {{ include('etudiant_annee_academique/_delete_form.html.twig') }} #}
{% endblock %}
