{% extends 'base.html.twig' %}

{% block title %}EtudiantAnneeAcademique index
{% endblock %}

{% block body %}
	<table class="table table-hover table-striped  table-bordered" id="dataTable" width="100%" cellspacing="0">
		<thead>
			<tr>
				<th>#</th>
				<th>Etudiant</th>
                <th>Matricule</th>
				<th>TelephoneTuteur</th>
				<th>HasReussie</th>
				<th>actions</th>
			</tr>
		</thead>
        <tfoot>
			<tr>
				<th>#</th>
				<th>Etudiant</th>
				<th>Matricule</th>
				<th>TelephoneTuteur</th>
				<th>HasReussie</th>
				<th>actions</th>
			</tr>
		</tfoot>
		<tbody>
			{% for etudiant_annee_academique in etudiant_annee_academiques %}
				<tr>
					<td>{{ loop.index }}</td>
					<td>{{ etudiant_annee_academique|upper }}</td>
					<td>{{ etudiant_annee_academique.matricule }}</td>
					<td>{{ etudiant_annee_academique.telephoneTuteur }}</td>
					<td>{{ etudiant_annee_academique.hasReussie ? 'Yes' : 'No' }}</td>
					<td>
						<a href="{{ path('app_etudiant_annee_academique_show', {'id': etudiant_annee_academique.id}) }}">show</a>
						<a href="{{ path('app_etudiant_annee_academique_edit', {'id': etudiant_annee_academique.id}) }}">edit</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="5">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock %}
